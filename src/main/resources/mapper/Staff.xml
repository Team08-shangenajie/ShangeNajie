<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        <mapper namespace="com.team.mapper.StaffMapper">
    <resultMap id="staffMap" type="com.team.domain.Staff">
    <id property="staffId" column="staff_id"
        javaType="java.lang.Integer"
        jdbcType="INTEGER"/>
    <result property="staffName" column="staff_name"/>
    <result property="staffPwd" column="staff_pwd"/>
</resultMap>
    <insert id="insertStaff" parameterType="com.team.domain.Staff"
            useGeneratedKeys="true" keyProperty="id" >
        INSERT  INTO staff(staffName,staffPwd) VALUES (#{staffName},#{staffPwd})
    </insert>

</mapper>